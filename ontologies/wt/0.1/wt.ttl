@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

<https://vocabularies.wholetale.org/wt/0.1/wt#>
  a owl:Ontology ;
  owl:versionIRI <https://vocabularies.wholetale.org/wt/0.1/wt#> ;
  rdfs:comment "This document describes a language for describing Tale related objects and the relations between them."@en ;
  rdfs:label "Whole Tale Ontology" ;
  owl:versionInfo "Version 0.1" .

<https://vocabularies.wholetale.org/wt/0.1/wt#usesDatasets>
  a owl:ObjectProperty ;
  rdfs:domain <https://vocabularies.wholetale.org/wt/0.1/wt#Tale> ;
  rdfs:range <https://schema.org/Dataset> ;
  rdfs:comment "Relates a Tale with a set of schema.org datasets."@en ;
  rdfs:label "Uses Datasets"@en .

<https://vocabularies.wholetale.org/wt/0.1/wt#internalIdentifier>
  a owl:ObjectProperty ;
  rdfs:domain <https://vocabularies.wholetale.org/wt/0.1/wt#Tale>, <http://www.openarchives.org/ore/terms/AggregatedResource> ;
  rdfs:range <http://www.w3.org/2000/01/rdf-schema#Literal> ;
  rdfs:comment "An identifier used by the internal Whole Tale system for resources."@en ;
  rdfs:label "Internal Identifier"@en .

<https://vocabularies.wholetale.org/wt/0.1/wt#TaleVersion>
  a owl:Class ;
  rdfs:subClassOf <https://schema.org/CreativeWork> ;
  rdfs:comment "A class that represents a particular version of a Tale."@en ;
  rdfs:label "Tale Version"@en .

<https://vocabularies.wholetale.org/wt/0.1/wt#Tale>
  a owl:Class ;
  rdfs:subClassOf <http://purl.org/wf4ever/ro#ResearchObject>, <https://schema.org/CreativeWork> ;
  rdfs:comment "A class that represents a Tale."@en ;
  rdfs:label "Tale"@en .
